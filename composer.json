{
    "name": "phpbenchmarks/phpixie-common",
    "type": "project",
    "description": "PHPixie skeleton project",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Project\\App\\": [
                "app/src/"
            ]
        }
    },
    "require": {
        "phpixie/default-bundle": "~3.2",
        "phpixie/bundle-framework": "~3.6",
